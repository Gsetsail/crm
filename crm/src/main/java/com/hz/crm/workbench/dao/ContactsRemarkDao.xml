<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hz.crm.workbench.dao.ContactsRemarkDao">

<insert id="insertOneContactsRemark">
    insert into tbl_contacts_remark(
            id,
            createTime,
            createBy,
            editFlag,
            contactsId,
            noteContent
            ) values(
            #{id},
            #{createTime},
            #{createBy},
            #{editFlag},
            #{contactsId},
            #{noteContent}
                                )
    </insert>
</mapper>

































